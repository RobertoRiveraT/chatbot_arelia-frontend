{
  "name": "frontend-webpage",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prebuild": "node scripts/write-env.js",
    "build:qa-staging": "cross-env NODE_ENV=staging npm run prebuild && ng build --configuration=qa-staging",
    "build:production": "cross-env NODE_ENV=production npm run prebuild && ng build --configuration=production",
    "serve:qa": "cross-env NODE_ENV=staging npm run prebuild && ng serve --configuration=qa-staging",
    "serve:production": "cross-env NODE_ENV=production-local npm run prebuild && ng serve --configuration=production",
    "build:vercel": "node scripts/build.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.11",
    "@angular/cli": "~15.2.11",
    "@angular/compiler-cli": "^15.2.10",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^18.15.11",
    "angular-cli-ghpages": "^2.0.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.5"
  }
}
